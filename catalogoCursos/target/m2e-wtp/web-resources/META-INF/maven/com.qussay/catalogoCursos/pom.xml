	<artifactId>catalogoCursos</artifactId>
	<version>1.0.0</version>
	<packaging>war</packaging>
	<name>Catalogo de cursos</name>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
